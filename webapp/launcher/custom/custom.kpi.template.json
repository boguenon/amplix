{
	"templates": [
		{
			"name": "Template 1",
			"value": "tmpl_1",
			"syntax": [
				"##define(col, {column})##",
				"##define(row, {row})##",
				"##define(_current_val, [$r,$c]) ##",
				"##define(_compare_val, {compare_index}) ##",
				"##define(_gap, $_current_val - $_compare_val) ##",
				"##define(_title, {title_index}) ##",
				"##define(_subtitle, [$r, 0]) ##",
				"<div class=\"idc-kpi-box\">",
				"    <table class=\"idc-kpi-table\">",
				"        <tr>",
				"            <td class=\"idc-kpi-tb-l0\" style=\"background-color:#29663D; text-align:center; vertical-align:middle;\">",
				"                <span class=\"{icon_css}\" style=\"font-family: font-awesome4; {icon_style}\"></span>",
				"            </td>",
				"            <td class=\"idc-kpi-tb-l1\" style=\"background-color: #52CC7A; line-height: 18px\">",
				"                <h2>",
				"                    <span style=\"##{$_gap > 0 ? \"color:red\" : \"color:green\"}##\">##{$_gap}##</span> ",
				"                    <span style=\"font-family: fontawesome\">##{$_gap > 0 ? \"&#xf062;\" : \"&#xf063;\"}##</span>",
				"                </h2>",
				"            <div style=\"font-size:28px; line-height: 28px\">##{$_title}##</div>",
				"               <div style=\"font-size: 15px; color: \">##{$_subtitle}## : <span style=\"color: red\">##{C$_current_val}##</span></div>",
				"               <div class=\"igc-kpi-micro-chart\" style=\"height: 60px\">##CHART(0)##</div></div>",
				"            </td>",
				"        </tr>",
				"    </table>",
				"</div>"
			],
			"parameters": [
				{
					"name": "row",
					"xtype": "textfield",
					"required": true,
					"emptyText": "$r or number",
					"value": "$r"
				},
				{
					"name": "column",
					"xtype": "textfield",
					"required": true,
					"emptyText": "$c or number",
					"value": "$c"
				},
				{
					"name": "title_index",
					"xtype": "textfield",
					"required": true,
					"emptyText": "[0, $c] or [$r, 0] or text",
					"value": "[0, $c]"
				},
				{
					"name": "subtitle_index",
					"xtype": "textfield",
					"emptyText": "[0, $c] or [$r, 0] or text",
					"value": "[$r, 0]"
				},
				{
					"name": "compare_index",
					"xtype": "textfield",
					"emptyText": "[$r + 1, $c] or empty",
					"value": "[$r + 1, $c]"
				},
				{
					"name": "icon_style",
					"xtype": "textfield",
					"emptyText": "fa-chart"
				},
				{
					"name": "icon_css",
					"xtype": "textfield",
					"value": "font-size: 80px;color:white"
				}
			]
		},
